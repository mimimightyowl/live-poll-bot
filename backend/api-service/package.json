{
  "name": "api-service",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "NODE_ENV=test ts-node-dev --respawn --transpile-only src/server.ts",
    "migrate": "node -r ts-node/register -r ./src/shared/env-loader node_modules/.bin/node-pg-migrate -m src/shared/infra/migrations up",
    "migrate:down": "node -r ts-node/register -r ./src/shared/env-loader node_modules/.bin/node-pg-migrate -m src/shared/infra/migrations down",
    "migrate:create": "node -r ts-node/register -r ./src/shared/env-loader node_modules/.bin/node-pg-migrate create -m src/shared/infra/migrations --migration-file-language sql",
    "seed:dev": "node -r ts-node/register -r ./src/shared/env-loader ./src/shared/infra/seeds/run-seed.ts",
    "db:reset": "npm run migrate:down && npm run migrate && npm run seed:dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=24.11.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.0",
    "@grpc/proto-loader": "^0.7.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.0",
    "@types/pg": "^8.11.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "node-pg-migrate": "^7.9.1",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0"
  }
}
